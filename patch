+    "def duration_arr(rec):\n",
+    "\n",
+    "    if rec < 300:\n",
+    "        x = 0 \n",
+    "    elif rec <= 400:\n",
+    "        x = 1\n",
+    "    elif rec <= 500:\n",
+    "        x = 2\n",
+    "    else:\n",
+    "        x= 3\n",
+    "    return x"
+   ]
